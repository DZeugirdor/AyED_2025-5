using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _1_AlbinoM_tp11
{
    class Program
    {
        static void Main(string[] args)
        {
            
            string numero = Console.ReadLine();
            int N;
            if (int.TryParse(numero, out N))
            for (int i = 1; i<=N; i++)

                if (i % 2 == 0)

                    Console.WriteLine(i);

               

            Console.ReadKey();
        }
    }
    
}
